{
	"name":"wasp/framework",
	"description": "A PHP framework designed for REST Apis and small Apps",
	"authors": [
		{
			"name":"Daniel Cox",
			"email":"danzabian@gmail.com",
			"homepage":"http://www.valeska.co.uk"
		}
	],
	"require": {
		"symfony/config": ">=2.7, <3.0",
		"symfony/console": ">=2.7, <3.0",
		"doctrine/common": "2.6.*dev",
		"symfony/proxy-manager-bridge": ">=2.7, <3.0",
		"symfony/dependency-injection": ">=2.7, <3.0",
		"symfony/filesystem": ">=2.7, <3.0",
		"symfony/http-foundation": ">=2.7, <3.0",
		"symfony/finder": ">=2.7, <3.0",
		"symfony/event-dispatcher": ">=2.7, <3.0",
		"symfony/http-kernel": ">=2.7, <3.0",
		"symfony/routing": ">=2.7, <3.0",
		"symfony/dom-crawler": ">=2.7, <3.0",
		"symfony/css-selector": ">=2.7, <3.0",
		"symfony/debug": ">=2.7, <3.0",
		"symfony/yaml": ">=2.7, <3.0",
		"symfony/security": ">=2.7, <3.0",
		"doctrine/orm": "*",
		"doctrine/data-fixtures": "2.0.*",
		"twig/twig": "1.*",
		"mockery/mockery": "dev-master",
		"symfony/templating": ">=2.7, <3.0",
		"symfony/validator": ">=2.7, <3.0",
		"jms/serializer": "0.17.*@dev"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"Wasp\\":"src/",
			"Wasp\\Test\\":"tests/"
		}
	}
}
